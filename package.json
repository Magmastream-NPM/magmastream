{
	"name": "magmastream",
	"version": "2.9.2-dev.3",
	"description": "A user-friendly Lavalink client designed for NodeJS.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"types": "rtb --dist dist",
		"lint": "eslint src",
		"lint:fix": "eslint --fix src",
		"ci": "run-s lint:fix lint build types",
		"release:dev": "npm run ci && npm version prerelease --preid=dev && npm run ci && npm publish --tag dev"
	},
	"devDependencies": {
		"@favware/rollup-type-bundler": "^4.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.20",
		"@types/node": "^22.16.5",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^8.37.0",
		"@typescript-eslint/parser": "^8.37.0",
		"eslint": "^9.31.0",
		"npm-run-all": "^4.1.5",
		"typedoc": "^0.27.9",
		"typedoc-plugin-no-inherit": "^1.6.1",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@discordjs/collection": "^2.1.1",
		"axios": "^1.10.0",
		"events": "^3.3.0",
		"ioredis": "^5.6.1",
		"jsdom": "^26.1.0",
		"lodash": "^4.17.21",
		"safe-stable-stringify": "^2.5.0",
		"tslib": "^2.8.1",
		"ws": "^8.18.3"
	},
	"optionalDependencies": {
		"detritus-client": "0.16.x",
		"discord.js": "14.x",
		"eris": "0.18.x",
		"oceanic.js": "1.12.0",
		"seyfert": "3.2.x"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"eslintConfig": {
		"root": true,
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"rules": {
			"object-curly-spacing": [
				"error",
				"always"
			]
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		]
	},
	"keywords": [
		"lavalink client",
		"wrapper",
		"typescript",
		"discord.js",
		"node.js",
		"java",
		"javascript",
		"audio streaming",
		"music bot",
		"voice chat",
		"discord integration",
		"high performance",
		"scalable",
		"easy-to-use",
		"feature-rich",
		"cross-platform",
		"seamless integration",
		"community support",
		"documentation",
		"open-source",
		"lavalink",
		"magmastream"
	],
	"repository": {
		"url": "git+https://github.com/Blackfort-Hosting/magmastream.git#main"
	},
	"homepage": "https://docs.magmastream.com",
	"author": "Abel Purnwasy",
	"license": "Apache-2.0"
}
